{
  "manifest_version": 3,
  "name": "DropoutHelper",
  "description": "Dropout but it saves your volume and subtitle settings",
  "version": "1.0",
  "action": {
    "default_icon": "dropout.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.dropout.tv/*"],
      "js": ["js/dropout.js"],
      "run_at": "document_end",
      "world": "MAIN"
    }
  ]
}
